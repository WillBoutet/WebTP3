
@{
    ViewBag.Title = "Accueil";
}

<h2>Accueil</h2>
<div class="jumbotron bg-dark">
    <div class="container text-center">
        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">

            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="film" src="https://upload.wikimedia.org/wikipedia/en/thumb/3/31/The_SpongeBob_SquarePants_Movie_poster.jpg/220px-The_SpongeBob_SquarePants_Movie_poster.jpg" alt="First slide">
                </div>
                <div class="carousel-item">
                    <img class="film" src="https://i.pinimg.com/originals/62/59/75/625975388a352bd8163810f2dc19730f.jpg" alt="Second slide">
                </div>
                <div class="carousel-item">
                    <img class="film" src="https://external-preview.redd.it/wWeCx6sAL1m8Choz7XjmCjT5w8DffyRkr_QGjv6j-8U.png?auto=webp&s=70b70af89416d22e7fffaf45881b774e68a33107" alt="Third slide">
                </div>
                <div class="carousel-item">
                    <img class="film" src="https://static.raru.co.za/cover/2017/02/14/5488643-l.jpg?v=1487060148" alt="Third slide">
                </div>
                <div class="carousel-item">
                    <img class="film" src="https://images-na.ssl-images-amazon.com/images/I/51kLRqFASAL._SY445_.jpg" alt="Third slide">
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
            <ol class="carousel-indicators">
                <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
            </ol>
        </div>
    </div>
</div>